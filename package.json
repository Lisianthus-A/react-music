{
    "name": "react-music",
    "version": "1.0.0",
    "private": true,
    "description": "基于 React + TypeScript 的仿网易云音乐播放器",
    "scripts": {
        "start": "concurrently \"npm run front\" \"npm run server\"",
        "front": "vite",
        "build": "tsc && vite build",
        "server": "node ./server/app.js",
        "deploy": "node ./server/app.js --deploy",
        "prestart": "node ./scripts/generateRoute.js",
        "prebuild": "node ./scripts/generateRoute.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lisianthus-a/react-music.git"
    },
    "keywords": [
        "react",
        "react hook",
        "music",
        "网易云音乐",
        "音乐",
        "网易云"
    ],
    "author": "Lisianthus-A",
    "license": "MIT",
    "dependencies": {
        "antd": "^5.4.6",
        "axios": "^0.25.0",
        "classnames": "^2.3.2",
        "express": "^4.17.2",
        "express-fileupload": "^1.2.1",
        "md5": "^2.3.0",
        "music-metadata": "^7.12.3",
        "pac-proxy-agent": "^5.0.0",
        "qrcode": "^1.5.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^6.2.1",
        "safe-decode-uri-component": "^1.2.1",
        "tunnel": "^0.0.6"
    },
    "devDependencies": {
        "@types/node": "^17.0.13",
        "@types/react": "^17.0.33",
        "@types/react-dom": "^17.0.10",
        "@vitejs/plugin-react": "^1.3.2",
        "concurrently": "^7.0.0",
        "sass": "^1.49.1",
        "typescript": "^4.7.4",
        "vite": "^3.1.0"
    }
}
