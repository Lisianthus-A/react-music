    "@types/react-dom": "^17.0.9",
    "@types/react-router": "^5.1.16",
    "@types/wicg-mediasession": "^1.1.3",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.3.0",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^5.2.7",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.4.1",
