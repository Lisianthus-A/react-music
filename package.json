{
    "name": "react-music",
    "version": "1.0.0",
    "private": true,
    "description": "基于 React + TypeScript 的仿网易云音乐播放器",
    "scripts": {
        "start": "concurrently \"npm run front\" \"npm run server\"",
        "front": "vite",
        "build": "tsc && vite build",
        "server": "node ./server/app.js",
        "prestart": "node ./scripts/generateRoute.js",
        "prebuild": "node ./scripts/generateRoute.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lisianthus-a/react-music.git"
    },
    "keywords": [
        "react",
        "react hook",
        "music",
        "网易云音乐",
        "音乐",
        "网易云"
    ],
    "author": "Lisianthus-A",
    "license": "MIT",
    "dependencies": {
        "@ant-design/icons": "^4.7.0",
        "antd": "^4.18.2",
        "axios": "^0.25.0",
        "express": "^4.17.2",
        "express-fileupload": "^1.2.1",
        "pac-proxy-agent": "^5.0.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^6.2.1",
        "request": "^2.88.2"
    },
    "devDependencies": {
        "@types/node": "^17.0.8",
        "@types/react": "^17.0.33",
        "@types/react-dom": "^17.0.10",
        "@vitejs/plugin-react": "^1.0.7",
        "concurrently": "^7.0.0",
        "sass": "^1.47.0",
        "typescript": "^4.4.4",
        "vite": "^2.7.2"
    }
}